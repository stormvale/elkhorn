{
  "name": "vite-template-redux",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "build:az-dev": "tsc -b && vite build --minify --mode development_az",
    "dev": "vite --open",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite --open",
    "test": "vitest --run",
    "type-check": "tsc -b --noEmit",
    "api-gen:restaurants": "npx @rtk-query/codegen-openapi src/features/restaurants/api/openapi-config.ts",
    "api-gen:schools": "npx @rtk-query/codegen-openapi src/features/schools/api/openapi-config.ts",
    "api-gen:lunches": "npx @rtk-query/codegen-openapi src/features/lunches/api/openapi-config.ts",
    "api-gen:users": "npx @rtk-query/codegen-openapi src/features/users/api/openapi-config.ts"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.15.0",
    "@azure/msal-react": "^3.0.15",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.6.0",
    "@mui/x-date-pickers": "^8.9.2",
    "@reduxjs/toolkit": "^2.6.1",
    "date-fns": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@rtk-query/codegen-openapi": "^2.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/eslint-plugin": "^1.1.39",
    "esbuild-runner": "^2.2.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^7.0.5",
    "vitest": "^3.1.1"
  }
}
