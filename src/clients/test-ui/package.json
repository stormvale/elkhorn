{
  "name": "vite-mui-ts",
  "version": "1.0.0",
  "description": "TypeScript + React + Redux + MUI + RRD + ESLint + Prettier",
  "license": "MIT",
  "keywords": [
    "react",
    "boilerplate",
    "typescript",
    "starter",
    "vite",
    "redux",
    "material-ui",
    "rrd",
    "prettier"
  ],
  "scripts": {
    "dev": "vite --open",
    "start": "vite --open",
    "host": "vite --open --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cover": "vitest run --coverage",
    "test:e2e": "pnpm preview:test 'cypress open'",
    "test:e2e:headless": "pnpm preview:test 'cypress run'",
    "test:e2e:ci": "vite build && pnpm preview:test 'cypress run --record'"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.15.0",
    "@azure/msal-react": "^3.0.15",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.9.0",
    "@reduxjs/toolkit": "^2.8.0",
    "axios": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "^9.12.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^16.3.0",
    "prettier": "^3.6.0",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.0"
  }
}
