        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "lunches-default": {
        "ClusterId": "lunches-cluster",
        "Match": { "Path": "lunches/{**catch-all}" },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "orders-default": {
        "ClusterId": "orders-cluster",
        "Match": { "Path": "orders/{**catch-all}" },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "notifications-default": {
        "ClusterId": "notifications-cluster",
        "Match": { "Path": "notifications/{**catch-all}" },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      }
    },
    "Clusters": {
      "restaurants-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://restaurants-api"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": "false",
            "Interval": "00:00:30",
            "Timeout": "00:00:10",
            "Policy": "ConsecutiveFailures",
            "Path": "/health"
          }
        }
      },
      "schools-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://schools-api"
          }
        }
      },
      "lunches-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://lunches-api"
          }
        }
      },
      "orders-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://orders-api"
          }
        }
      },
      "notifications-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://notifications-api"
          }
        }
      }
    }
  }
}
